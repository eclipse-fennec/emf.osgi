
-enable-gecko-emf: true


# Build dependencies 
-buildpath: \
	org.geckoprojects.emf.example.model.basic;version=latest,\
	org.geckoprojects.emf.repository.api;version=latest,\
	org.geckoprojects.emf.repository.file;version=latest


Bundle-Version: 2.0.15.SNAPSHOT
Private-Package: org.geckoprojects.emf.repository.tests

-resolve.effective: active;skip:="osgi.service"

# Needed for Mockito's mocking to work
-runsystempackages.objenesis: sun.misc,sun.reflect

# Use Felix by default
-runfw: org.apache.felix.framework;version='[7.0.0,7.0.0]'
-runvm: -ea
-runrequires: \
	bnd.identity;id='org.geckoprojects.emf.repository.tests'
-runee: JavaSE-1.8