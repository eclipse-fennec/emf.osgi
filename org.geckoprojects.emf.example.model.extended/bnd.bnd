src=${^src},src-gen

-buildpath: \
	org.geckoprojects.emf.osgi.api;version=latest,\
	org.geckoprojects.emf.osgi.annotation;version=latest,\
	org.geckoprojects.emf.example.model.basic;version=latest,\
	org.eclipse.emf.ecore,\
	org.eclipse.emf.common,\
	org.osgi.annotation.versioning,\
	org.eclipse.emf.ecore.xmi,\
	org.geckoprojects.emf.osgi.api,\
	org.osgi.annotation.bundle,\
	org.osgi.service.component.annotations,\
	org.osgi.framework
	
-baseline: *

-includeresource: model=model

Bundle-Version: 0.0.1-SNAPSHOT

Export-Package: \
	org.geckoprojects.emf.example.model.extended.model.extendend,\
	org.geckoprojects.emf.example.model.extended.model.extendend.impl,\
	org.geckoprojects.emf.example.model.extended.model.extendend.util
	
	
-dependson: org.geckoprojects.emf.codegen

-generate:\
	model/extendend.genmodel;\
		generate="geckoEMF 1>out.log 2>err.log";\
		genmodel=model/extendend.genmodel;\
		output=src-gen
-privatepackage: org.geckoprojects.emf.example.model.extended.model.configuration