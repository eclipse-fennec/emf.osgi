<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated basic -->
      <package
            uri="http://gecko.org/example/model/basic"
            class="org.gecko.emf.osgi.example.model.basic.BasicPackage"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.content_parser">
      <!-- @generated basic -->
      <parser
            contentTypeIdentifier="basic#1.0"
            class="org.gecko.emf.osgi.example.model.basic.util.BasicResourceFactoryImpl"/>
   </extension>

   <extension point="org.eclipse.core.contenttype.contentTypes">
      <!-- @generated basic -->
      <content-type
            base-type="org.eclipse.emf.ecore.xmi"
            file-extensions="basic"
            id="basic#1.0"
            name="%_UI_Basic_content_type"
            priority="normal">
         <describer class="org.eclipse.emf.ecore.xmi.impl.RootXMLContentHandlerImpl$Describer">
            <parameter name="namespace" value="http://gecko.org/example/model/basic"/>
            <parameter name="kind" value="xmi"/>
         </describer>
      </content-type>
   </extension>

</plugin>
