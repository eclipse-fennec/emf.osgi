# Build dependencies 
-buildpath: \
	org.geckoprojects.emf.core;version=project,\
	org.geckoprojects.emf.core.model.info;version=project,\
	org.geckoprojects.emf.example.model.extended;version=project,\
	org.geckoprojects.example.model.basic;version=project,\
	junit-jupiter-api,\
	org.osgi.test.junit5,\
	org.osgi.framework,\
	org.eclipse.emf.ecore,\
	org.osgi.test.common,\
	assertj-core,\
	org.osgi.service.cm,\
	org.geckoprojects.osgitest;version=latest,\
	org.geckoprojects.emf.example.model.basic,\
	org.eclipse.emf.common,\
	org.osgi.util.promise,\
	org.geckoprojects.emf.model.info,\
	org.osgi.annotation.versioning

Bundle-Version: ${emf-version}

-includeresource: \
	data/=data/

Bundle-Name: EMF OSGi Integration Tests
Bundle-Description: OSGI tests for the EMF OSGi stuff
Export-Package: org.geckoprojects.emf.tests
Private-Package: org.geckoprojects.emf.tests.configurator
